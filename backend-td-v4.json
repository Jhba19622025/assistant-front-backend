      "environment": [
        { "name": "NODE_ENV", "value": "production" },
        { "name": "OPENAI_API_KEY", "value": "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" },
        { "name": "ASSISTANCE_ID",  "value": "asst_CzCSiymQdoOe93a8GrGzt3Ge" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/iaapp-backend",
